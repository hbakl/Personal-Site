@model MvcCv.Models.Entity.TblSkill
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
</br>
<h4> Yeni Yetenek Ekleme Sayfası</h4>
</br>


<form method="post">

    @Html.Label("Yetenek Açıklaması")
    @Html.TextBoxFor(x => x.Skill, new { @class = "form-control" })
    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Rate, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Kaydet</button>
</form>